{
  "expo": {
    "name": "techappnewdev",
    "slug": "techappnewdev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "techappnewdev",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to find and connect to nearby devices.",
        "NSBluetoothPeripheralUsageDescription": "This app needs Bluetooth access to communicate with nearby devices."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.rufflesteels.techappnewdev"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-ble-plx",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "android": {
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ],
      "package": "com.rufflesteels.techappnewdev"
    }
  }
}
